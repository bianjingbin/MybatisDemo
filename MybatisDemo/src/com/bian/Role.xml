<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bian.DeptMapper">
	<select id="getDept" parameterType="map" resultType="Integer">
		select dept_id from dept where 1=1 and dept_id=#{id}
	</select>
</mapper>